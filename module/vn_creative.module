hostname = %APPEND% api.revenuecat.com
#Picsart
hostname = %APPEND% api.picsart.c*, api.meiease.c*
#SoundCloud
hostname= %APPEND% api-mobile.soundcloud.com
#Spotify
hostname = %APPEND% spclient.wg.spotify.com
#Youtube
